{# @var array constituenciesElections -#}

<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
    {% for constituencySlug, electionSlugs in constituenciesElections %}
        {% for electionSlug, _dummy in electionSlugs %}
        <url>
            <loc>{{ url('constituency_election', {'slug': constituencySlug, 'electionSlug': electionSlug}) }}</loc>
            <changefreq>monthly</changefreq>
        </url>
        {% endfor %}
    {% endfor %}
</urlset>