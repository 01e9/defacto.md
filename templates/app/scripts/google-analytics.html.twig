{# @var string id #}

<script async src="https://www.googletagmanager.com/gtag/js?id={{ id|escape('html_attr') }}"></script>
<script>
    window.dataLayer = window.dataLayer || [];
    function gtag(){dataLayer.push(arguments);}
    gtag('js', new Date());
    gtag('config', '{{ id|escape('js') }}');
</script>